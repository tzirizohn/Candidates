@using Course.Data
@model IEnumerable<Candidate>


<table class="table table-hover table-striped table-bordered" style="margin-top: 15px">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone Number</th>
    </tr>

    @foreach (Candidate c in Model)
    {
        <tr>
            <td>@c.FirstName</td>
            <td>@c.LastName</td>
            <td>@c.Email</td>
            <td>@c.PhoneNumber</td>
        </tr>
    }

</table>
